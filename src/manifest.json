{
  "name": "Zeropool",
  "version": "1.0",
  "description": "Zeropool chrome extension!",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "background.js",
      "runtime.js",
      "vendor.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "note": "https://stackoverflow.com/questions/48523118/wasm-module-compile-error-in-chrome-extension"
}
