{
  "name": "Zeropool",
  "version": "1.0",
  "description": "Zeropool chrome extension!",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "background.js",
      "runtime.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html"
  }
}
